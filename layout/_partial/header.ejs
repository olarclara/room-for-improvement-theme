<header id="header">
  <a id="title" href="<%- url_for("/") %>">
    <h1><%= config.title %></h1>
  </a>
  <ul id="nav">
    <% for (var i in theme.nav) { %> 
      <li><a href="<%- url_for(theme.nav[i]) %>"><%= i %></a></li>
    <% } %>
  </ul>
  <p id="description"><%= config.description %></p>
  <% if (theme.social) { %>
    <div id="social">
      <p>Find me on </p>
      <ul>
        <% for(var i in theme.social) { %>
          <li><a target="_blank" href="<%- url_for(theme.social[i]) %>"><%- i %></a></li>
        <% } %>
      </ul>
    </div>
  <% } %>
</header>